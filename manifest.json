{
  "name": "kibela-UML-opener",
  "manifest_version": 2,
  "version": "1.0.1",
  "browser_action": {
    "default_title": "BaclkgroundColor"
  },

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  } ,

  "content_scripts": [
    {
      "matches": ["https://*.kibe.la/notes/*"],
      "js": ["main.js"]
    }
  ]
}
